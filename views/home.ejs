<body>
  <main style="margin: 1em;">
    <h3>Public Quizes</h3>
    <table class="table">
      <tbody>
        <% for (let quiz of quizzes) { %>
          <tr class="row">
            <td class="quiz-name"><form method="GET" action="quiz/<%= quiz.id %>">
              <button type="submit" class="btn btn-outline-primary quiz-body"><%= quiz.title %></button>
            </form></td>
            <td class="score"> Most recent score: <%= quiz.id %></td>
          </tr>
        <% } %>
      </tbody>
    </table>
  </main>
